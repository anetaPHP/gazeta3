{% include 'default/_form_article.html.twig' with {
    page_title:  'label.editstory' ,
    page_title_param: {'%id%': article.id},
    attr: {
        method: 'PUT',
        action: url('article_edit', {'id': article.id}),
    },
    back_to_list_path: 'app_admini',
    submit_label: 'label.save'|trans ,
} %}


{% block after_javascripts %}
    <script src="{{ asset('js/article_form.js') }}"></script>
    <script>
        jQuery(document).ready(function () {
            initCollection("{{ 'label.add'|trans }}", "{{ 'label.delete'|trans }}");
        });
    </script>
{% endblock %}